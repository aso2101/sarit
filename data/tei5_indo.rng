    <element name="divGen">
      <ref name="attlist.divGen"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.divGen" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(document author) contains the name of the author of the document, as given on the
    title page (often but not always contained in a byline).
  -->
  <define name="docAuthor">
    <element name="docAuthor">
      <ref name="attlist.docAuthor"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.docAuthor" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.canonical.attributes"/>
  </define>
  <!--
    doc:(document date) contains the date of a document, as given
    (usually) on a title page.
  -->
  <define name="docDate">
    <element name="docDate">
      <ref name="attlist.docDate"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.docDate" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="when"/>
    </optional>
  </define>
  <!--
    doc:(document edition) contains an edition statement as presented on a title page of a
    document.
  -->
  <define name="docEdition">
    <element name="docEdition">
      <ref name="attlist.docEdition"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.docEdition" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(document imprint) contains the imprint statement (place and date of publication,
    publisher name), as given
    (usually) at the foot of a title page.
  -->
  <define name="docImprint">
    <element name="docImprint">
      <ref name="attlist.docImprint"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="model.phrase"/>
          <ref name="pubPlace"/>
          <ref name="docDate"/>
          <ref name="publisher"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.docImprint" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(document title) contains the title of a document, including all its
    constituents, as given on a title page.
  -->
  <define name="docTitle">
    <element name="docTitle">
      <ref name="attlist.docTitle"/>
      <zeroOrMore>
        <ref name="model.global"/>
      </zeroOrMore>
      <oneOrMore>
        <ref name="titlePart"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.docTitle" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.canonical.attributes"/>
  </define>
  <!--
    doc:(domain of use) describes the most important social context in which the text was
    realized or for which it is intended, for example private vs. public,
    education, religion, etc.
  -->
  <define name="domain">
    <element name="domain">
      <ref name="attlist.domain"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.domain" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!-- doc:(edition) describes the particularities of one edition of a text. -->
  <define name="edition">
    <element name="edition">
      <ref name="attlist.edition"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.edition" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:(edition statement) groups information relating to one edition of a text. -->
  <define name="editionStmt">
    <element name="editionStmt">
      <ref name="attlist.editionStmt"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <ref name="edition"/>
          <zeroOrMore>
            <ref name="respStmt"/>
          </zeroOrMore>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.editionStmt" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:secondary statement of responsibility for a
    bibliographic item, for example the name of an individual, institution
    or organization, (or of several such) acting as editor, compiler,
    translator, etc.
  -->
  <define name="editor">
    <element name="editor">
      <ref name="attlist.editor"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.editor" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="role" a:defaultValue="editor">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(editorial practice declaration) provides details of editorial principles and practices applied
    during the encoding of a text.
  -->
  <define name="editorialDecl">
    <element name="editorialDecl">
      <ref name="attlist.editorialDecl"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="model.editorialDeclPart"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.editorialDecl" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!-- doc:contains a description of the educational experience of a person. -->
  <define name="education">
    <element name="education">
      <ref name="attlist.education"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.education" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.naming.attributes"/>
  </define>
  <!--
    doc:(electronic mail address) contains an e-mail address identifying a location to which
    e-mail messages can be delivered.
  -->
  <define name="email">
    <element name="email">
      <ref name="attlist.email"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.email" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(emphasized) marks words or phrases which are stressed or emphasized for
    linguistic or rhetorical effect.
  -->
  <define name="emph">
    <element name="emph">
      <ref name="attlist.emph"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.emph" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(encoding description) documents the relationship between an electronic text and the
    source or sources from which it was derived.
  -->
  <define name="encodingDesc">
    <element name="encodingDesc">
      <ref name="attlist.encodingDesc"/>
      <oneOrMore>
        <choice>
          <ref name="model.encodingPart"/>
          <ref name="model.pLike"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.encodingDesc" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains a quotation, anonymous or attributed, appearing at the start of a section or
    chapter, or on a title page.
  -->
  <define name="epigraph">
    <element name="epigraph">
      <ref name="attlist.epigraph"/>
      <zeroOrMore>
        <choice>
          <ref name="model.common"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.epigraph" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(event) contains data relating to any kind of significant event associated with a person, place, or
    organization.
  -->
  <define name="event">
    <element name="event">
      <ref name="attlist.event"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="model.labelLike"/>
        </oneOrMore>
      </choice>
      <zeroOrMore>
        <choice>
          <ref name="model.noteLike"/>
          <ref name="model.biblLike"/>
        </choice>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="event"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.event" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.naming.attributes"/>
    <optional>
      <attribute name="where"/>
    </optional>
  </define>
  <!--
    doc:(editorial expansion) contains a sequence of letters added by an editor or
    transcriber when expanding an abbreviation.
  -->
  <define name="ex">
    <element name="ex">
      <ref name="attlist.ex"/>
      <ref name="macro.xtext"/>
    </element>
  </define>
  <define name="attlist.ex" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.editLike.attributes"/>
  </define>
  <!-- doc:(expansion) contains the expansion of an abbreviation. -->
  <define name="expan">
    <element name="expan">
      <ref name="attlist.expan"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.expan" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.editLike.attributes"/>
  </define>
  <!--
    doc:contains the  explicit of a
    manuscript item, that is, the closing words of the text proper,
    exclusive of any rubric or colophon which might follow it.
  -->
  <define name="explicit">
    <element name="explicit">
      <ref name="attlist.explicit"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.explicit" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.msExcerpt.attributes"/>
  </define>
  <!--
    doc:describes the approximate size of a text as stored on
    some carrier medium, whether digital or non-digital, specified in any convenient units.
  -->
  <define name="extent">
    <element name="extent">
      <ref name="attlist.extent"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.extent" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains a representation of some written source in the form of
    a set of images rather than as transcribed or encoded text.
  -->
  <define name="facsimile">
    <element name="facsimile">
      <ref name="attlist.facsimile"/>
      <optional>
        <ref name="front"/>
      </optional>
      <oneOrMore>
        <choice>
          <ref name="model.graphicLike"/>
          <ref name="surface"/>
        </choice>
      </oneOrMore>
      <optional>
        <ref name="back"/>
      </optional>
    </element>
  </define>
  <define name="attlist.facsimile" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declaring.attributes"/>
  </define>
  <!--
    doc:describes the extent to which the text may be regarded as
    imaginative or non-imaginative, that is, as describing a fictional
    or a non-fictional world.
  -->
  <define name="factuality">
    <element name="factuality">
      <ref name="attlist.factuality"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.factuality" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="type">
        <choice>
          <value>fiction</value>
          <value>fact</value>
          <value>mixed</value>
          <value>inapplicable</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!-- doc:specifies the faith, religion, or belief set of a person. -->
  <define name="faith">
    <element name="faith">
      <ref name="attlist.faith"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.faith" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.datable.attributes"/>
  </define>
  <!--
    doc:(description of figure) contains a brief prose description of the appearance or content
    of a graphic figure, for use when documenting an image without
    displaying it.
  -->
  <define name="figDesc">
    <element name="figDesc">
      <ref name="attlist.figDesc"/>
      <ref name="macro.limitedContent"/>
    </element>
  </define>
  <define name="attlist.figDesc" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:groups elements representing or containing graphic information such as an illustration or
    figure.
  -->
  <define name="figure">
    <element name="figure">
      <ref name="attlist.figure"/>
      <zeroOrMore>
        <choice>
          <ref name="model.headLike"/>
          <ref name="model.pLike"/>
          <ref name="figDesc"/>
          <ref name="model.graphicLike"/>
          <ref name="_DUMMY_model.egLike"/>
          <ref name="floatingText"/>
          <ref name="figure"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.figure" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.placement.attributes"/>
  </define>
  <!--
    doc:(file description) contains a full bibliographic description of an electronic
    file.
  -->
  <define name="fileDesc">
    <element name="fileDesc">
      <ref name="attlist.fileDesc"/>
      <group>
        <ref name="titleStmt"/>
        <optional>
          <ref name="editionStmt"/>
        </optional>
        <optional>
          <ref name="extent"/>
        </optional>
        <ref name="publicationStmt"/>
        <optional>
          <ref name="seriesStmt"/>
        </optional>
        <optional>
          <ref name="notesStmt"/>
        </optional>
      </group>
      <oneOrMore>
        <ref name="sourceDesc"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.fileDesc" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:contains information concerning the manuscript's filiation, i.e. its relationship to other surviving manuscripts of the same text, its protographs, antigraphs and apographs. -->
  <define name="filiation">
    <element name="filiation">
      <ref name="attlist.filiation"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.filiation" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!-- doc:contains the string of words that denotes the end of a text division, often with an assertion as to its author and title, usually set off from the text itself by red ink, by a different size or type of script, or by some other such visual device. -->
  <define name="finalRubric">
    <element name="finalRubric">
      <ref name="attlist.finalRubric"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.finalRubric" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:contains a single text of any kind, whether unitary or composite, which interrupts the text
    containing it at any point and after which the surrounding text resumes.
  -->
  <define name="floatingText">
    <element name="floatingText">
      <ref name="attlist.floatingText"/>
      <zeroOrMore>
        <ref name="model.global"/>
      </zeroOrMore>
      <optional>
        <ref name="front"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </optional>
      <choice>
        <ref name="body"/>
        <ref name="group"/>
      </choice>
      <zeroOrMore>
        <ref name="model.global"/>
      </zeroOrMore>
      <optional>
        <ref name="back"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </optional>
    </element>
  </define>
  <define name="attlist.floatingText" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declaring.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!-- doc:contains information about a person's period of activity. -->
  <define name="floruit">
    <element name="floruit">
      <ref name="attlist.floruit"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.floruit" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
  </define>
  <!--
    doc:describes the numbering system or systems used to 
    count the leaves or pages in a codex.
  -->
  <define name="foliation">
    <element name="foliation">
      <ref name="attlist.foliation"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.foliation" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(foreign) identifies a word or phrase as belonging to some language other
    than that of the surrounding text.
  -->
  <define name="foreign">
    <element name="foreign">
      <ref name="attlist.foreign"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.foreign" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:contains a forename, given or baptismal name. -->
  <define name="forename">
    <element name="forename">
      <ref name="attlist.forename"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.forename" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.personal.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!-- doc:contains a mathematical or other formula. -->
  <define name="formula">
    <element name="formula">
      <ref name="attlist.formula"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="model.graphicLike"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.formula" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="notation"/>
    </optional>
  </define>
  <!--
    doc:(front matter) contains any prefatory matter (headers,
    title page, prefaces, dedications, etc.)
    found  at the start of a document, before the main body.
  -->
  <define name="front">
    <element name="front">
      <ref name="attlist.front"/>
      <zeroOrMore>
        <choice>
          <ref name="model.frontPart"/>
          <ref name="model.pLike.front"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
      <optional>
        <choice>
          <group>
            <ref name="model.div1Like"/>
            <zeroOrMore>
              <choice>
                <ref name="model.frontPart"/>
                <ref name="model.div1Like"/>
                <ref name="model.global"/>
              </choice>
            </zeroOrMore>
          </group>
          <group>
            <ref name="model.divLike"/>
            <zeroOrMore>
              <choice>
                <ref name="model.frontPart"/>
                <ref name="model.divLike"/>
                <ref name="model.global"/>
              </choice>
            </zeroOrMore>
          </group>
        </choice>
      </optional>
      <optional>
        <ref name="model.divBottomPart"/>
        <zeroOrMore>
          <choice>
            <ref name="model.divBottomPart"/>
            <ref name="model.global"/>
          </choice>
        </zeroOrMore>
      </optional>
    </element>
  </define>
  <define name="attlist.front" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declaring.attributes"/>
  </define>
  <!--
    doc:(funding body) specifies the name of an individual, institution, or organization
    responsible for the funding of a project or text.
  -->
  <define name="funder">
    <element name="funder">
      <ref name="attlist.funder"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.funder" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(forme work) contains a running head (e.g. a header, footer), catchword, or
    similar material appearing on the current page.
  -->
  <define name="fw">
    <element name="fw">
      <ref name="attlist.fw"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.fw" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.placement.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(gap) indicates a point where material has been omitted in a
    transcription, whether for editorial reasons described in the TEI
    header, as part of sampling practice, or because the material is
    illegible, invisible, or inaudible.
  -->
  <define name="gap">
    <element name="gap">
      <ref name="attlist.gap"/>
      <zeroOrMore>
        <ref name="model.glossLike"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.gap" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.editLike.attributes"/>
    <optional>
      <attribute name="reason"/>
    </optional>
    <optional>
      <attribute name="hand"/>
    </optional>
    <optional>
      <attribute name="agent">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(generational name component) contains a name component used to distinguish otherwise similar names on the basis of the
    relative ages or generations of the persons named.
  -->
  <define name="genName">
    <element name="genName">
      <ref name="attlist.genName"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.genName" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.personal.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(geographical coordinates) contains any expression of a set of geographic coordinates, representing a point, line, or
    area on the surface of the earth in some notation.
  -->
  <define name="geo">
    <element name="geo">
      <ref name="attlist.geo"/>
      <text/>
    </element>
  </define>
  <define name="attlist.geo" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(geographic coordinates declaration) documents the notation and the datum used for geographic
    coordinates expressed as content of the geo element
    elsewhere within the document.
  -->
  <define name="geoDecl">
    <element name="geoDecl">
      <ref name="attlist.geoDecl"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.geoDecl" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
    <optional>
      <attribute name="datum" a:defaultValue="WGS84">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(geographical feature name) contains a common noun identifying some geographical feature contained within a geographic
    name, such as valley, mount, etc.
  -->
  <define name="geogFeat">
    <element name="geogFeat">
      <ref name="attlist.geogFeat"/>
      <ref name="macro.xtext"/>
    </element>
  </define>
  <define name="attlist.geogFeat" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.naming.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.datable.attributes"/>
  </define>
  <!-- doc:(geographical name) a name associated with some geographical feature such as Windrush Valley or Mount Sinai. -->
  <define name="geogName">
    <element name="geogName">
      <ref name="attlist.geogName"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.geogName" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.naming.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:identifies a phrase or word used to provide a gloss or
    definition for some other word or phrase.
  -->
  <define name="gloss">
    <element name="gloss">
      <ref name="attlist.gloss"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.gloss" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declaring.attributes"/>
    <ref name="att.translatable.attributes"/>
    <ref name="att.typed.attributes"/>
    <optional>
      <attribute name="target"/>
    </optional>
    <optional>
      <attribute name="cRef"/>
    </optional>
  </define>
  <!-- doc:indicates the location of an inline graphic, illustration, or figure. -->
  <define name="graphic">
    <element name="graphic">
      <ref name="attlist.graphic"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.graphic" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.internetMedia.attributes"/>
    <ref name="att.declaring.attributes"/>
    <optional>
      <attribute name="width"/>
    </optional>
    <optional>
      <attribute name="height"/>
    </optional>
    <optional>
      <attribute name="scale"/>
    </optional>
    <optional>
      <attribute name="url"/>
    </optional>
  </define>
  <!--
    doc:contains the body of a composite text, grouping together a sequence of distinct texts (or
    groups of such texts) which are regarded as a unit for some purpose, for example the collected
    works of an author, a sequence of prose essays, etc.
  -->
  <define name="group">
    <element name="group">
      <ref name="attlist.group"/>
      <zeroOrMore>
        <choice>
          <ref name="model.divTop"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
      <group>
        <choice>
          <ref name="text"/>
          <ref name="group"/>
        </choice>
        <zeroOrMore>
          <choice>
            <ref name="text"/>
            <ref name="group"/>
            <ref name="model.global"/>
          </choice>
        </zeroOrMore>
      </group>
      <zeroOrMore>
        <ref name="model.divBottom"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.group" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declaring.attributes"/>
  </define>
  <!-- doc:(description of hands) contains a description of all the different kinds of writing  used in a manuscript. -->
  <define name="handDesc">
    <element name="handDesc">
      <ref name="attlist.handDesc"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <optional>
            <ref name="summary"/>
          </optional>
          <oneOrMore>
            <ref name="handNote"/>
          </oneOrMore>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.handDesc" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="hands"/>
    </optional>
  </define>
  <!--
    doc:(note on hand) describes a particular style
    or hand distinguished within a manuscript.
  -->
  <define name="handNote">
    <element name="handNote">
      <ref name="attlist.handNote"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.handNote" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.handFeatures.attributes"/>
  </define>
  <!--
    doc:contains one or more handNote elements documenting the
    different hands identified within the source texts.
  -->
  <define name="handNotes">
    <element name="handNotes">
      <ref name="attlist.handNotes"/>
      <oneOrMore>
        <ref name="handNote"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.handNotes" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:marks the beginning of a sequence of text written in a new
    hand, or the beginning of a scribal stint.
  -->
  <define name="handShift">
    <element name="handShift">
      <ref name="attlist.handShift"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.handShift" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.handFeatures.attributes"/>
    <optional>
      <attribute name="new"/>
    </optional>
    <optional>
      <attribute name="resp"/>
    </optional>
  </define>
  <!--
    doc:(heading) contains any type of heading, for example the title of a section,
    or the heading of a list, glossary, manuscript description, etc.
  -->
  <define name="head">
    <element name="head">
      <ref name="attlist.head"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.head" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(heading for list items) contains the heading for the item or gloss column in a glossary list or similar structured
    list.
  -->
  <define name="headItem">
    <element name="headItem">
      <ref name="attlist.headItem"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.headItem" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(heading for list labels) contains the heading for the label or term column in a
    glossary list or similar structured list.
  -->
  <define name="headLabel">
    <element name="headLabel">
      <ref name="attlist.headLabel"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.headLabel" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:contains a measurement measured along the axis parallel to the spine. -->
  <define name="height">
    <element name="height">
      <ref name="attlist.height"/>
      <ref name="macro.xtext"/>
    </element>
  </define>
  <define name="attlist.height" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.dimensions.attributes"/>
  </define>
  <!--
    doc:contains a heraldic formula
    or phrase, typically found as part of a blazon, coat of arms, etc. 
  -->
  <define name="heraldry">
    <element name="heraldry">
      <ref name="attlist.heraldry"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.heraldry" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(highlighted) marks a word or phrase as graphically distinct from the
    surrounding text, for reasons concerning which no claim is
    made.
  -->
  <define name="hi">
    <element name="hi">
      <ref name="attlist.hi"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.hi" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc: groups elements
    describing the full history of a manuscript or manuscript part.
  -->
  <define name="history">
    <element name="history">
      <ref name="attlist.history"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <optional>
            <ref name="summary"/>
          </optional>
          <optional>
            <ref name="origin"/>
          </optional>
          <zeroOrMore>
            <ref name="provenance"/>
          </zeroOrMore>
          <optional>
            <ref name="acquisition"/>
          </optional>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.history" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:summarizes the way in which hyphenation in a source text has been
    treated in an encoded version of it.
  -->
  <define name="hyphenation">
    <element name="hyphenation">
      <ref name="attlist.hyphenation"/>
      <oneOrMore>
        <ref name="model.pLike"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.hyphenation" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
    <optional>
      <attribute name="eol" a:defaultValue="some">
        <choice>
          <value>all</value>
          <value>some</value>
          <value>hard</value>
          <value>none</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(identifying number) supplies any standard or non-standard number used to identify a
    bibliographic item.
  -->
  <define name="idno">
    <element name="idno">
      <ref name="attlist.idno"/>
      <text/>
    </element>
  </define>
  <define name="attlist.idno" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:contains a formal statement authorizing the publication of a work, sometimes required to
    appear on a title page or its verso.
  -->
  <define name="imprimatur">
    <element name="imprimatur">
      <ref name="attlist.imprimatur"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.imprimatur" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:groups information relating to the publication or distribution
    of a bibliographic item.
  -->
  <define name="imprint">
    <element name="imprint">
      <ref name="attlist.imprint"/>
      <oneOrMore>
        <choice>
          <ref name="model.imprintPart"/>
          <ref name="model.dateLike"/>
        </choice>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.imprint" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:contains the incipit of a manuscript item, that is the opening words of the text proper, exclusive of any rubric which might precede it, of sufficient length to identify the work uniquely; such incipts were, in fomer times, frequently used a means of reference to a work, in place of a title. -->
  <define name="incipit">
    <element name="incipit">
      <ref name="attlist.incipit"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.incipit" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.msExcerpt.attributes"/>
  </define>
  <!-- doc:(index entry) marks a location to be indexed for whatever purpose. -->
  <define name="index">
    <element name="index">
      <ref name="attlist.index"/>
      <zeroOrMore>
        <ref name="term"/>
        <optional>
          <ref name="index"/>
        </optional>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.index" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.spanning.attributes"/>
    <optional>
      <attribute name="indexName">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:contains the name of an organization such as a university or
    library, with which a manuscript is identified, generally its
    holding institution.
  -->
  <define name="institution">
    <element name="institution">
      <ref name="attlist.institution"/>
      <ref name="macro.xtext"/>
    </element>
  </define>
  <define name="attlist.institution" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.naming.attributes"/>
  </define>
  <!--
    doc:describes the extent, cardinality and nature of any interaction
    among those producing and experiencing the text, for example in the
    form of response or interjection, commentary, etc.
  -->
  <define name="interaction">
    <element name="interaction">
      <ref name="attlist.interaction"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.interaction" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="type">
        <choice>
          <value>none</value>
          <value>partial</value>
          <value>complete</value>
          <value>inapplicable</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="active">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="passive">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(interpretation) summarizes a specific interpretative annotation which can be
    linked to a span of text.
  -->
  <define name="interp">
    <element name="interp">
      <ref name="attlist.interp"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="model.glossLike"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.interp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.interpLike.attributes"/>
  </define>
  <!--
    doc:(interpretation group) collects together a set of related interpretations which share
    responsibility or type.
  -->
  <define name="interpGrp">
    <element name="interpGrp">
      <ref name="attlist.interpGrp"/>
      <zeroOrMore>
        <ref name="model.glossLike"/>
      </zeroOrMore>
      <oneOrMore>
        <ref name="interp"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.interpGrp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.interpLike.attributes"/>
  </define>
  <!--
    doc:describes the scope of any analytic or interpretive information
    added to the text in addition to the transcription.
  -->
  <define name="interpretation">
    <element name="interpretation">
      <ref name="attlist.interpretation"/>
      <oneOrMore>
        <ref name="model.pLike"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.interpretation" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:contains one
    component of a list.
  -->
  <define name="item">
    <element name="item">
      <ref name="attlist.item"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.item" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:identifies a possibly fragmented segment of text, by pointing
    at the possibly discontiguous elements which compose it.
  -->
  <define name="join">
    <element name="join">
      <ref name="attlist.join"/>
      <zeroOrMore>
        <ref name="model.glossLike"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.join" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.pointing.attributes"/>
    <attribute name="targets"/>
    <optional>
      <attribute name="result">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="scope" a:defaultValue="root">
        <choice>
          <value>root</value>
          <value>branches</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(join group) groups a collection of join elements and possibly
    pointers.
  -->
  <define name="joinGrp">
    <element name="joinGrp">
      <ref name="attlist.joinGrp"/>
      <zeroOrMore>
        <ref name="model.glossLike"/>
      </zeroOrMore>
      <oneOrMore>
        <choice>
          <ref name="join"/>
          <ref name="ptr"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.joinGrp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.pointing.group.attributes"/>
    <optional>
      <attribute name="result">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:contains a list of keywords or phrases identifying the topic or
    nature of a text.
  -->
  <define name="keywords">
    <element name="keywords">
      <ref name="attlist.keywords"/>
      <choice>
        <oneOrMore>
          <ref name="term"/>
        </oneOrMore>
        <ref name="list"/>
      </choice>
    </element>
  </define>
  <define name="attlist.keywords" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="scheme"/>
  </define>
  <!-- doc:(verse line) contains a single, possibly incomplete, line of verse. -->
  <define name="l">
    <element name="l">
      <ref name="attlist.l"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.l" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.metrical.attributes"/>
    <ref name="att.enjamb.attributes"/>
    <optional>
      <attribute name="part" a:defaultValue="N">
        <choice>
          <value>Y</value>
          <value>N</value>
          <value>I</value>
          <value>M</value>
          <value>F</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!--
    doc:contains the label associated with an item in a list; in glossaries, marks the term being
    defined.
  -->
  <define name="label">
    <element name="label">
      <ref name="attlist.label"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.label" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:indicates the end of a lacuna in a mostly complete textual
    witness.
  -->
  <define name="lacunaEnd">
    <element name="lacunaEnd">
      <ref name="attlist.lacunaEnd"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.lacunaEnd" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.rdgPart.attributes"/>
  </define>
  <!--
    doc:indicates the beginning of a lacuna in the text of a mostly
    complete textual witness.
  -->
  <define name="lacunaStart">
    <element name="lacunaStart">
      <ref name="attlist.lacunaStart"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.lacunaStart" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.rdgPart.attributes"/>
  </define>
  <!--
    doc:(language knowledge) summarizes the state of a person's linguistic knowledge, either as prose or by a list of
    langKnown elements.
  -->
  <define name="langKnowledge">
    <element name="langKnowledge">
      <ref name="attlist.langKnowledge"/>
      <choice>
        <ref name="model.pLike"/>
        <oneOrMore>
          <ref name="langKnown"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.langKnowledge" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <optional>
      <attribute name="tags"/>
    </optional>
  </define>
  <!--
    doc:(language known) summarizes the state of a person's linguistic competence, i.e., knowledge of a single
    language.
  -->
  <define name="langKnown">
    <element name="langKnown">
      <ref name="attlist.langKnown"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.langKnown" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <attribute name="tag"/>
    <optional>
      <attribute name="level"/>
    </optional>
  </define>
  <!--
    doc:(language usage) describes the languages, sublanguages, registers, dialects, etc.
    represented within a text.
  -->
  <define name="langUsage">
    <element name="langUsage">
      <ref name="attlist.langUsage"/>
      <oneOrMore>
        <ref name="language"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.langUsage" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:characterizes a single language or sublanguage used within a
    text.
  -->
  <define name="language">
    <element name="language">
      <ref name="attlist.language"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.language" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="ident"/>
    <optional>
      <attribute name="usage"/>
    </optional>
  </define>
  <!--
    doc:describes how text is laid out on the
    page, including information about any ruling, pricking, or other
    evidence of page-preparation techniques.
  -->
  <define name="layout">
    <element name="layout">
      <ref name="attlist.layout"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.layout" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="columns"/>
    </optional>
    <optional>
      <attribute name="ruledLines"/>
    </optional>
    <optional>
      <attribute name="writtenLines"/>
    </optional>
  </define>
  <!-- doc:(layout description) collects the set of layout descriptions applicable to a manuscript. -->
  <define name="layoutDesc">
    <element name="layoutDesc">
      <ref name="attlist.layoutDesc"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="layout"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.layoutDesc" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:(line break) marks the start of a new (typographic) line in some edition or version of a text. -->
  <define name="lb">
    <element name="lb">
      <ref name="attlist.lb"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.lb" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.sourced.attributes"/>
  </define>
  <!-- doc:(lemma) contains the lemma, or base text, of a textual variation. -->
  <define name="lem">
    <element name="lem">
      <ref name="attlist.lem"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="model.phrase"/>
          <ref name="model.inter"/>
          <ref name="model.global"/>
          <ref name="model.rdgPart"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.lem" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.textCritical.attributes"/>
  </define>
  <!--
    doc:(line group) contains a group of verse lines functioning as a formal unit, e.g. a stanza, refrain,
    verse paragraph, etc.
  -->
  <define name="lg">
    <element name="lg">
      <ref name="attlist.lg"/>
      <zeroOrMore>
        <choice>
          <ref name="model.divTop"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
      <choice>
        <ref name="model.lLike"/>
        <ref name="lg"/>
      </choice>
      <zeroOrMore>
        <choice>
          <ref name="model.lLike"/>
          <ref name="lg"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="model.divBottom"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.lg" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.divLike.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.declaring.attributes"/>
  </define>
  <!--
    doc:defines an association or hypertextual link
    among elements or passages, of some type
    not more precisely specifiable by other elements.
  -->
  <define name="link">
    <element name="link">
      <ref name="attlist.link"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.link" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.pointing.attributes"/>
    <attribute name="targets"/>
  </define>
  <!-- doc:(link group) defines a collection of associations or hypertextual links. -->
  <define name="linkGrp">
    <element name="linkGrp">
      <ref name="attlist.linkGrp"/>
      <oneOrMore>
        <choice>
          <ref name="link"/>
          <ref name="ptr"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.linkGrp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.pointing.group.attributes"/>
  </define>
  <!-- doc:(list) contains any sequence of items organized as a list. -->
  <define name="list">
    <element name="list">
      <ref name="attlist.list"/>
      <zeroOrMore>
        <choice>
          <ref name="model.divTop"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
      <choice>
        <oneOrMore>
          <ref name="item"/>
          <zeroOrMore>
            <ref name="model.global"/>
          </zeroOrMore>
        </oneOrMore>
        <group>
          <optional>
            <ref name="headLabel"/>
          </optional>
          <optional>
            <ref name="headItem"/>
          </optional>
          <oneOrMore>
            <ref name="label"/>
            <zeroOrMore>
              <ref name="model.global"/>
            </zeroOrMore>
            <ref name="item"/>
            <zeroOrMore>
              <ref name="model.global"/>
            </zeroOrMore>
          </oneOrMore>
        </group>
      </choice>
      <zeroOrMore>
        <ref name="model.divBottom"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.list" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="type" a:defaultValue="simple">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!-- doc:(citation list) contains a list of bibliographic citations of any kind. -->
  <define name="listBibl">
    <element name="listBibl">
      <ref name="attlist.listBibl"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <oneOrMore>
        <choice>
          <ref name="model.biblLike"/>
          <ref name="model.milestoneLike"/>
          <ref name="listBibl"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.listBibl" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc: (list of events) contains a list of descriptions, each of which provides
    information about an identifiable event.
  -->
  <define name="listEvent">
    <element name="listEvent">
      <ref name="attlist.listEvent"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <oneOrMore>
        <choice>
          <ref name="event"/>
          <ref name="listEvent"/>
        </choice>
      </oneOrMore>
      <zeroOrMore>
        <choice>
          <ref name="relation"/>
          <ref name="relationGrp"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.listEvent" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!-- doc:(list of canonical names) contains a list of nyms, that is, standardized names for any thing. -->
  <define name="listNym">
    <element name="listNym">
      <ref name="attlist.listNym"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <oneOrMore>
        <choice>
          <ref name="nym"/>
          <ref name="listNym"/>
        </choice>
      </oneOrMore>
      <zeroOrMore>
        <choice>
          <ref name="relationGrp"/>
          <ref name="relation"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.listNym" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:(list of organizations) contains a list of elements, each of which provides information about an identifiable
    organization.
  -->
  <define name="listOrg">
    <element name="listOrg">
      <ref name="attlist.listOrg"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <oneOrMore>
        <choice>
          <ref name="org"/>
          <ref name="listOrg"/>
        </choice>
      </oneOrMore>
      <zeroOrMore>
        <choice>
          <ref name="relationGrp"/>
          <ref name="relation"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.listOrg" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:(list of persons) contains a list of descriptions, each of which provides information about an identifiable
    person or a group of people, for example the participants in a language interaction, or the
    people referred to in a historical source.
  -->
  <define name="listPerson">
    <element name="listPerson">
      <ref name="attlist.listPerson"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <oneOrMore>
        <choice>
          <ref name="model.personLike"/>
          <ref name="listPerson"/>
        </choice>
      </oneOrMore>
      <zeroOrMore>
        <choice>
          <ref name="relation"/>
          <ref name="relationGrp"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.listPerson" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:(list of places) contains a list of places, optionally followed by a list of relationships (other than
    containment) defined amongst them.
  -->
  <define name="listPlace">
    <element name="listPlace">
      <ref name="attlist.listPlace"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <oneOrMore>
        <choice>
          <ref name="model.placeLike"/>
          <ref name="listPlace"/>
        </choice>
      </oneOrMore>
      <zeroOrMore>
        <choice>
          <ref name="relationGrp"/>
          <ref name="relation"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.listPlace" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:(witness list) lists definitions for all the witnesses referred to by a critical
    apparatus, optionally grouped hierarchically.
  -->
  <define name="listWit">
    <element name="listWit">
      <ref name="attlist.listWit"/>
      <optional>
        <ref name="model.headLike"/>
      </optional>
      <oneOrMore>
        <choice>
          <ref name="witness"/>
          <ref name="listWit"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.listWit" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains a brief informal description of the kind of
    place concerned, for example: a room, a restaurant, a park bench, etc.
  -->
  <define name="locale">
    <element name="locale">
      <ref name="attlist.locale"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.locale" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:defines the location of a place as a set of geographical coordinates, in terms of a other named geo-political entities, or as an
    address.
  -->
  <define name="location">
    <element name="location">
      <ref name="attlist.location"/>
      <zeroOrMore>
        <choice>
          <ref name="model.labelLike"/>
          <ref name="model.placeNamePart"/>
          <ref name="model.offsetLike"/>
          <ref name="model.measureLike"/>
          <ref name="model.addressLike"/>
          <ref name="model.noteLike"/>
          <ref name="model.biblLike"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.location" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
  </define>
  <!-- doc:defines a location  within a manuscript or manuscript part, usually as a (possibly discontinuous) sequence of folio references. -->
  <define name="locus">
    <element name="locus">
      <ref name="attlist.locus"/>
      <ref name="macro.xtext"/>
    </element>
  </define>
  <define name="attlist.locus" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="scheme"/>
    </optional>
    <optional>
      <attribute name="from"/>
    </optional>
    <optional>
      <attribute name="to"/>
    </optional>
    <optional>
      <attribute name="target"/>
    </optional>
  </define>
  <!--
    doc:groups a number of locations which together form a
    distinct but discontinuous item within a manuscript or manuscript
    part, according to a specific foliation.
  -->
  <define name="locusGrp">
    <element name="locusGrp">
      <ref name="attlist.locusGrp"/>
      <oneOrMore>
        <ref name="locus"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.locusGrp" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="scheme"/>
    </optional>
  </define>
  <!-- doc:(morpheme) represents a grammatical morpheme. -->
  <define name="m">
    <element name="m">
      <ref name="attlist.m"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="model.segLike"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.m" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.segLike.attributes"/>
    <ref name="att.typed.attributes"/>
    <optional>
      <attribute name="baseForm"/>
    </optional>
  </define>
  <!--
    doc:contains a word or phrase describing the
    material of which  a manuscript (or part of a manuscript) is composed.
  -->
  <define name="material">
    <element name="material">
      <ref name="attlist.material"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.material" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains a word or phrase referring to some quantity of an object or commodity, usually
    comprising a number, a unit, and a commodity name.
  -->
  <define name="measure">
    <element name="measure">
      <ref name="attlist.measure"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.measure" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.measurement.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(measure group) contains a group of dimensional specifications which relate to the same object, for example
    the height and width of a manuscript page.
  -->
  <define name="measureGrp">
    <element name="measureGrp">
      <ref name="attlist.measureGrp"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="model.measureLike"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.measureGrp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.measurement.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:contains the formalized descriptive title for a meeting or conference, for use in a
    bibliographic description for an item derived from such a meeting, or as a heading or
    preamble to publications emanating from it.
  -->
  <define name="meeting">
    <element name="meeting">
      <ref name="attlist.meeting"/>
      <ref name="macro.limitedContent"/>
    </element>
  </define>
  <define name="attlist.meeting" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:marks words or phrases mentioned, not used. -->
  <define name="mentioned">
    <element name="mentioned">
      <ref name="attlist.mentioned"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.mentioned" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(metrical notation declaration) documents the notation employed to represent a metrical
    pattern when this is specified as the value of a met,
    real, or rhyme attribute on any structural
    element of a metrical text (e.g. lg, l, or
    seg).
  -->
  <define name="metDecl">
    <element name="metDecl">
      <ref name="attlist.metDecl"/>
      <choice>
        <oneOrMore>
          <choice>
            <ref name="model.pLike"/>
            <ref name="model.noteLike"/>
          </choice>
        </oneOrMore>
        <oneOrMore>
          <ref name="metSym"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.metDecl" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
    <optional>
      <attribute name="type" a:defaultValue="met real">
        <data type="NMTOKENS"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="pattern"/>
    </optional>
  </define>
  <!--
    doc:(metrical notation symbol) documents the intended significance of a particular character or
    character sequence within a metrical notation, either explicitly or in
    terms of other symbol elements in the same metDecl.
  -->
  <define name="metSym">
    <element name="metSym">
      <ref name="attlist.metSym"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.metSym" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="value"/>
    <optional>
      <attribute name="terminal" a:defaultValue="true"/>
    </optional>
  </define>
  <!--
    doc:marks a boundary point separating any kind of section of a text, typically but not
    necessarily indicating a point at which some part of a standard reference system
    changes, where the change is not represented by a structural element.
  -->
  <define name="milestone">
    <element name="milestone">
      <ref name="attlist.milestone"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.milestone" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.sourced.attributes"/>
    <attribute name="unit">
      <data type="NMTOKEN"/>
    </attribute>
  </define>
  <!--
    doc:(monographic level) contains bibliographic elements describing an item (e.g. a book or journal) published as
    an independent item (i.e. as a separate physical object).
  -->
  <define name="monogr">
    <element name="monogr">
      <ref name="attlist.monogr"/>
      <optional>
        <choice>
          <group>
            <choice>
              <ref name="author"/>
              <ref name="editor"/>
              <ref name="respStmt"/>
            </choice>
            <zeroOrMore>
              <choice>
                <ref name="author"/>
                <ref name="editor"/>
                <ref name="respStmt"/>
              </choice>
            </zeroOrMore>
            <oneOrMore>
              <ref name="title"/>
            </oneOrMore>
            <zeroOrMore>
              <choice>
                <ref name="idno"/>
                <ref name="editor"/>
                <ref name="respStmt"/>
              </choice>
            </zeroOrMore>
          </group>
          <group>
            <oneOrMore>
              <ref name="title"/>
            </oneOrMore>
            <zeroOrMore>
              <choice>
                <ref name="idno"/>
                <ref name="author"/>
                <ref name="editor"/>
                <ref name="respStmt"/>
              </choice>
            </zeroOrMore>
          </group>
        </choice>
      </optional>
      <zeroOrMore>
        <choice>
          <ref name="model.noteLike"/>
          <ref name="meeting"/>
        </choice>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="edition"/>
        <zeroOrMore>
          <choice>
            <ref name="idno"/>
            <ref name="editor"/>
            <ref name="respStmt"/>
          </choice>
        </zeroOrMore>
      </zeroOrMore>
      <ref name="imprint"/>
      <zeroOrMore>
        <choice>
          <ref name="imprint"/>
          <ref name="extent"/>
          <ref name="biblScope"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.monogr" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(manuscript contents) describes the intellectual content of
    a manuscript or manuscript part, either as a series of paragraphs or
    as a series of structured manuscript items.
  -->
  <define name="msContents">
    <element name="msContents">
      <ref name="attlist.msContents"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <optional>
            <ref name="summary"/>
          </optional>
          <optional>
            <ref name="textLang"/>
          </optional>
          <optional>
            <ref name="titlePage"/>
          </optional>
          <zeroOrMore>
            <choice>
              <ref name="msItem"/>
              <ref name="msItemStruct"/>
            </choice>
          </zeroOrMore>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.msContents" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.msExcerpt.attributes"/>
    <optional>
      <attribute name="class"/>
    </optional>
  </define>
  <!--
    doc:(manuscript description) contains a description of a single identifiable
    manuscript.
  -->
  <define name="msDesc">
    <element name="msDesc">
      <ref name="attlist.msDesc"/>
      <ref name="msIdentifier"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <optional>
            <ref name="msContents"/>
          </optional>
          <optional>
            <ref name="physDesc"/>
          </optional>
          <optional>
            <ref name="history"/>
          </optional>
          <optional>
            <ref name="additional"/>
          </optional>
          <zeroOrMore>
            <ref name="msPart"/>
          </zeroOrMore>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.msDesc" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(manuscript identifier) contains the information required to identify
    the  manuscript being described.
  -->
  <define name="msIdentifier">
    <element name="msIdentifier">
      <ref name="attlist.msIdentifier"/>
      <choice>
        <group>
          <ref name="model.placeNamePart_sequenceOptional"/>
          <optional>
            <ref name="institution"/>
          </optional>
          <ref name="repository"/>
          <optional>
            <ref name="collection"/>
          </optional>
          <optional>
            <ref name="idno"/>
          </optional>
        </group>
        <ref name="msName"/>
      </choice>
      <zeroOrMore>
        <choice>
          <ref name="altIdentifier"/>
          <ref name="msName"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.msIdentifier" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(manuscript item) describes an individual work or item within the intellectual
    content of a manuscript or manuscript part.
  -->
  <define name="msItem">
    <element name="msItem">
      <ref name="attlist.msItem"/>
      <zeroOrMore>
        <choice>
          <ref name="locus"/>
          <ref name="locusGrp"/>
        </choice>
      </zeroOrMore>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <choice>
            <ref name="model.titlepagePart"/>
            <ref name="model.msItemPart"/>
            <ref name="model.global"/>
          </choice>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.msItem" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.msExcerpt.attributes"/>
    <optional>
      <attribute name="class"/>
    </optional>
  </define>
  <!--
    doc:(structured manuscript item) contains a structured description for an
    individual work or item within the intellectual content of a
    manuscript or manuscript part.
  -->
  <define name="msItemStruct">
    <element name="msItemStruct">
      <ref name="attlist.msItemStruct"/>
      <optional>
        <ref name="locus"/>
      </optional>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <zeroOrMore>
            <ref name="author"/>
          </zeroOrMore>
          <zeroOrMore>
            <ref name="respStmt"/>
          </zeroOrMore>
          <zeroOrMore>
            <ref name="title"/>
          </zeroOrMore>
          <optional>
            <ref name="rubric"/>
          </optional>
          <optional>
            <ref name="incipit"/>
          </optional>
          <zeroOrMore>
            <ref name="msItemStruct"/>
          </zeroOrMore>
          <optional>
            <ref name="explicit"/>
          </optional>
          <optional>
            <ref name="finalRubric"/>
          </optional>
          <zeroOrMore>
            <ref name="colophon"/>
          </zeroOrMore>
          <zeroOrMore>
            <ref name="decoNote"/>
          </zeroOrMore>
          <zeroOrMore>
            <ref name="listBibl"/>
          </zeroOrMore>
          <zeroOrMore>
            <ref name="bibl"/>
          </zeroOrMore>
          <zeroOrMore>
            <ref name="filiation"/>
          </zeroOrMore>
          <zeroOrMore>
            <ref name="model.noteLike"/>
          </zeroOrMore>
          <optional>
            <ref name="textLang"/>
          </optional>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.msItemStruct" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.msExcerpt.attributes"/>
    <optional>
      <attribute name="class"/>
    </optional>
  </define>
  <!--
    doc:(alternative name) contains any form of unstructured alternative
    name used for a manuscript, such as an  ocellus
    nominum, or nickname.
  -->
  <define name="msName">
    <element name="msName">
      <ref name="attlist.msName"/>
      <ref name="macro.xtext"/>
    </element>
  </define>
  <define name="attlist.msName" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(manuscript part) contains information about an originally
    distinct manuscript or part of a manuscript, now forming part of a composite manuscript.
  -->
  <define name="msPart">
    <element name="msPart">
      <ref name="attlist.msPart"/>
      <ref name="altIdentifier"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <optional>
            <ref name="msContents"/>
          </optional>
          <optional>
            <ref name="physDesc"/>
          </optional>
          <optional>
            <ref name="history"/>
          </optional>
          <optional>
            <ref name="additional"/>
          </optional>
          <zeroOrMore>
            <ref name="msPart"/>
          </zeroOrMore>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.msPart" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:contains description of type of musical notation. -->
  <define name="musicNotation">
    <element name="musicNotation">
      <ref name="attlist.musicNotation"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.musicNotation" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:(name, proper noun) contains a proper noun or noun phrase. -->
  <define name="name">
    <element name="name">
      <ref name="attlist.name"/>
