      <zeroOrMore>
        <choice>
          <ref name="model.global"/>
          <ref name="model.headLike"/>
        </choice>
      </zeroOrMore>
      <oneOrMore>
        <ref name="model.common"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.argument" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:in a bibliographic reference, contains the name of the author(s), personal or corporate, of
    a work; the primary statement of responsibility for any bibliographic item.
  -->
  <define name="author">
    <element name="author">
      <ref name="attlist.author"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.author" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.canonical.attributes"/>
  </define>
  <!--
    doc:(release authority) supplies the name of a person or other agency responsible for
    making an electronic file available, other than a publisher or
    distributor.
  -->
  <define name="authority">
    <element name="authority">
      <ref name="attlist.authority"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.authority" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:supplies information about the availability of a text, for
    example any restrictions on its use or distribution, its copyright
    status, etc.
  -->
  <define name="availability">
    <element name="availability">
      <ref name="attlist.availability"/>
      <oneOrMore>
        <ref name="model.pLike"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.availability" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
    <optional>
      <attribute name="status" a:defaultValue="unknown">
        <choice>
          <value>free</value>
          <value>unknown</value>
          <value>restricted</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!-- doc:(back matter) contains any appendixes, etc. following the main part of a text. -->
  <define name="back">
    <element name="back">
      <ref name="attlist.back"/>
      <zeroOrMore>
        <choice>
          <ref name="model.frontPart"/>
          <ref name="model.pLike.front"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
      <optional>
        <choice>
          <group>
            <ref name="model.div1Like"/>
            <zeroOrMore>
              <choice>
                <ref name="model.frontPart"/>
                <ref name="model.div1Like"/>
                <ref name="model.global"/>
              </choice>
            </zeroOrMore>
          </group>
          <group>
            <ref name="model.divLike"/>
            <zeroOrMore>
              <choice>
                <ref name="model.frontPart"/>
                <ref name="model.divLike"/>
                <ref name="model.global"/>
              </choice>
            </zeroOrMore>
          </group>
        </choice>
      </optional>
      <optional>
        <ref name="model.divBottomPart"/>
        <zeroOrMore>
          <choice>
            <ref name="model.divBottomPart"/>
            <ref name="model.global"/>
          </choice>
        </zeroOrMore>
      </optional>
    </element>
  </define>
  <define name="attlist.back" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declaring.attributes"/>
  </define>
  <!--
    doc:(bibliographic citation) contains a loosely-structured bibliographic citation of which the sub-components may or may
    not be explicitly tagged.
  -->
  <define name="bibl">
    <element name="bibl">
      <ref name="attlist.bibl"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="model.highlighted"/>
          <ref name="model.pPart.data"/>
          <ref name="model.pPart.edit"/>
          <ref name="model.segLike"/>
          <ref name="model.ptrLike"/>
          <ref name="model.biblPart"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.bibl" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(fully-structured bibliographic citation) contains a fully-structured bibliographic citation, in which all
    components of the TEI file description
    are present.
  -->
  <define name="biblFull">
    <element name="biblFull">
      <ref name="attlist.biblFull"/>
      <group>
        <ref name="titleStmt"/>
        <optional>
          <ref name="editionStmt"/>
        </optional>
        <optional>
          <ref name="extent"/>
        </optional>
        <ref name="publicationStmt"/>
        <optional>
          <ref name="seriesStmt"/>
        </optional>
        <optional>
          <ref name="notesStmt"/>
        </optional>
      </group>
      <zeroOrMore>
        <ref name="sourceDesc"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.biblFull" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:(scope of citation) defines the scope of a bibliographic reference, for example as a
    list of page numbers, or a named subdivision of a larger work.
  -->
  <define name="biblScope">
    <element name="biblScope">
      <ref name="attlist.biblScope"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.biblScope" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:(structured bibliographic citation) contains a structured bibliographic citation, in which only bibliographic sub-elements
    appear and in a specified order.
  -->
  <define name="biblStruct">
    <element name="biblStruct">
      <ref name="attlist.biblStruct"/>
      <zeroOrMore>
        <ref name="analytic"/>
      </zeroOrMore>
      <oneOrMore>
        <ref name="monogr"/>
        <zeroOrMore>
          <ref name="series"/>
        </zeroOrMore>
      </oneOrMore>
      <zeroOrMore>
        <choice>
          <ref name="model.noteLike"/>
          <ref name="idno"/>
          <ref name="relatedItem"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.biblStruct" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!-- doc:provides encoded binary data representing an inline graphic or other object. -->
  <define name="binaryObject">
    <element name="binaryObject">
      <ref name="attlist.binaryObject"/>
      <text/>
    </element>
  </define>
  <define name="attlist.binaryObject" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.internetMedia.attributes"/>
    <optional>
      <attribute name="width"/>
    </optional>
    <optional>
      <attribute name="height"/>
    </optional>
    <optional>
      <attribute name="scale"/>
    </optional>
    <optional>
      <attribute name="encoding"/>
    </optional>
  </define>
  <!-- doc:contains a description of one binding, i.e. type of covering, boards, etc. applied to a manuscript. -->
  <define name="binding">
    <element name="binding">
      <ref name="attlist.binding"/>
      <oneOrMore>
        <choice>
          <ref name="model.pLike"/>
          <ref name="condition"/>
          <ref name="decoNote"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.binding" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <optional>
      <attribute name="contemporary"/>
    </optional>
  </define>
  <!--
    doc:(binding description) describes the present and former bindings of a manuscript, either
    as a series of paragraphs or as a series of distinct binding elements,
    one for each binding of the manuscript.
  -->
  <define name="bindingDesc">
    <element name="bindingDesc">
      <ref name="attlist.bindingDesc"/>
      <choice>
        <oneOrMore>
          <choice>
            <ref name="model.pLike"/>
            <ref name="decoNote"/>
            <ref name="condition"/>
          </choice>
        </oneOrMore>
        <oneOrMore>
          <ref name="binding"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.bindingDesc" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:(birth) contains information about a person's birth, such as its date and place. -->
  <define name="birth">
    <element name="birth">
      <ref name="attlist.birth"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.birth" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.naming.attributes"/>
  </define>
  <!--
    doc:(bloc) contains the name of a geo-political unit consisting of two or more nation states or
    countries.
  -->
  <define name="bloc">
    <element name="bloc">
      <ref name="attlist.bloc"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.bloc" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.naming.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.datable.attributes"/>
  </define>
  <!-- doc:(text body) contains the whole body of a single unitary text, excluding any front or back matter. -->
  <define name="body">
    <element name="body">
      <ref name="attlist.body"/>
      <zeroOrMore>
        <ref name="model.global"/>
      </zeroOrMore>
      <optional>
        <ref name="model.divTop"/>
        <zeroOrMore>
          <choice>
            <ref name="model.global"/>
            <ref name="model.divTop"/>
          </choice>
        </zeroOrMore>
      </optional>
      <optional>
        <ref name="model.divGenLike"/>
        <zeroOrMore>
          <choice>
            <ref name="model.global"/>
            <ref name="model.divGenLike"/>
          </choice>
        </zeroOrMore>
      </optional>
      <choice>
        <oneOrMore>
          <ref name="model.divLike"/>
          <zeroOrMore>
            <choice>
              <ref name="model.global"/>
              <ref name="model.divGenLike"/>
            </choice>
          </zeroOrMore>
        </oneOrMore>
        <oneOrMore>
          <ref name="model.div1Like"/>
          <zeroOrMore>
            <choice>
              <ref name="model.global"/>
              <ref name="model.divGenLike"/>
            </choice>
          </zeroOrMore>
        </oneOrMore>
        <group>
          <oneOrMore>
            <ref name="model.common"/>
            <zeroOrMore>
              <ref name="model.global"/>
            </zeroOrMore>
          </oneOrMore>
          <optional>
            <choice>
              <oneOrMore>
                <ref name="model.divLike"/>
                <zeroOrMore>
                  <choice>
                    <ref name="model.global"/>
                    <ref name="model.divGenLike"/>
                  </choice>
                </zeroOrMore>
              </oneOrMore>
              <oneOrMore>
                <ref name="model.div1Like"/>
                <zeroOrMore>
                  <choice>
                    <ref name="model.global"/>
                    <ref name="model.divGenLike"/>
                  </choice>
                </zeroOrMore>
              </oneOrMore>
            </choice>
          </optional>
        </group>
      </choice>
      <zeroOrMore>
        <ref name="model.divBottom"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.body" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declaring.attributes"/>
  </define>
  <!--
    doc:contains the primary statement of responsibility given for a work
    on its title page or at the head or end of the work.
  -->
  <define name="byline">
    <element name="byline">
      <ref name="attlist.byline"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="model.phrase"/>
          <ref name="docAuthor"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.byline" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:(character) represents a character. -->
  <define name="c">
    <element name="c">
      <ref name="attlist.c"/>
      <ref name="macro.xtext"/>
    </element>
  </define>
  <define name="attlist.c" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.segLike.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(canonical reference pattern) specifies an expression and replacement pattern for
    transforming a canonical reference into a URI.
  -->
  <define name="cRefPattern">
    <element name="cRefPattern">
      <ref name="attlist.cRefPattern"/>
      <zeroOrMore>
        <ref name="model.pLike"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.cRefPattern" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="matchPattern"/>
    <attribute name="replacementPattern"/>
  </define>
  <!-- doc:marks the point at which a metrical line may be divided. -->
  <define name="caesura">
    <element name="caesura">
      <ref name="attlist.caesura"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.caesura" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(category description) describes some category within a taxonomy
    or text typology, either in the form of a brief prose description
    or in terms of the situational parameters used by the TEI
    formal textDesc.
  -->
  <define name="catDesc">
    <element name="catDesc">
      <ref name="attlist.catDesc"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="model.limitedPhrase"/>
          <ref name="model.catDescPart"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.catDesc" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(category reference) specifies one or more defined categories
    within some taxonomy or text typology.
  -->
  <define name="catRef">
    <element name="catRef">
      <ref name="attlist.catRef"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.catRef" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="target"/>
    <optional>
      <attribute name="scheme"/>
    </optional>
  </define>
  <!--
    doc:describes the system used to ensure
    correct ordering of the quires making up a codex or incunable,
    typically by means of annotations at the foot of the page.
  -->
  <define name="catchwords">
    <element name="catchwords">
      <ref name="attlist.catchwords"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.catchwords" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains an individual descriptive category, possibly nested
    within a superordinate category, within a user-defined taxonomy.
  -->
  <define name="category">
    <element name="category">
      <ref name="attlist.category"/>
      <choice>
        <ref name="catDesc"/>
        <zeroOrMore>
          <ref name="model.glossLike"/>
        </zeroOrMore>
      </choice>
      <zeroOrMore>
        <ref name="category"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.category" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(column break) marks the boundary between one column of a text and the next
    in a standard reference system.
  -->
  <define name="cb">
    <element name="cb">
      <ref name="attlist.cb"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.cb" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.sourced.attributes"/>
  </define>
  <!-- doc:contains one cell of a table. -->
  <define name="cell">
    <element name="cell">
      <ref name="attlist.cell"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.cell" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.tableDecoration.attributes"/>
  </define>
  <!--
    doc:indicates the degree of certainty or uncertainty associated
    with some aspect of the text markup.
  -->
  <define name="certainty">
    <element name="certainty">
      <ref name="attlist.certainty"/>
      <zeroOrMore>
        <ref name="model.glossLike"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.certainty" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="target"/>
    <attribute name="locus">
      <data type="NMTOKEN"/>
    </attribute>
    <optional>
      <attribute name="assertedValue"/>
    </optional>
    <optional>
      <attribute name="given"/>
    </optional>
    <optional>
      <attribute name="degree"/>
    </optional>
  </define>
  <!--
    doc:summarizes a particular change or correction made
    to a particular version of an electronic text which is
    shared between several researchers.
  -->
  <define name="change">
    <element name="change">
      <ref name="attlist.change"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="model.limitedPhrase"/>
          <ref name="model.inter"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.change" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.ascribed.attributes"/>
    <optional>
      <attribute name="when"/>
    </optional>
  </define>
  <!--
    doc:(primary channel) describes the medium or channel by which a text is delivered or
    experienced. For a written text, this might be print, manuscript, e-mail, etc.;
    for a spoken one, radio, telephone, face-to-face, etc.
  -->
  <define name="channel">
    <element name="channel">
      <ref name="attlist.channel"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.channel" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="mode" a:defaultValue="x">
        <choice>
          <value>s</value>
          <value>w</value>
          <value>sw</value>
          <value>ws</value>
          <value>m</value>
          <value>x</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!--
    doc:groups a number of alternative encodings for the same point in
    a text.
  -->
  <define name="choice">
    <element name="choice">
      <ref name="attlist.choice"/>
      <zeroOrMore>
        <choice>
          <ref name="model.choicePart"/>
          <ref name="choice"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.choice" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(cited quotation) contains a quotation from some other document, together with a
    bibliographic reference to its source.  In a dictionary it may
    contain an example text with at least one occurrence of the word
    form, used in the sense being described, or a translation of the
    headword, or an example.
  -->
  <define name="cit">
    <element name="cit">
      <ref name="attlist.cit"/>
      <oneOrMore>
        <choice>
          <ref name="model.qLike"/>
          <ref name="model.biblLike"/>
          <ref name="model.ptrLike"/>
          <ref name="model.global"/>
          <ref name="_DUMMY_model.entryPart"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.cit" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!-- doc:(clause) represents a grammatical clause. -->
  <define name="cl">
    <element name="cl">
      <ref name="attlist.cl"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.cl" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.segLike.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(classification code) contains the classification code used for this text in some
    standard classification system.
  -->
  <define name="classCode">
    <element name="classCode">
      <ref name="attlist.classCode"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.classCode" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="scheme"/>
  </define>
  <!--
    doc:(classification declarations) contains one or more taxonomies defining any classificatory
    codes used elsewhere in the text.
  -->
  <define name="classDecl">
    <element name="classDecl">
      <ref name="attlist.classDecl"/>
      <oneOrMore>
        <ref name="taxonomy"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.classDecl" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:(climate) contains information about the physical climate of a place. -->
  <define name="climate">
    <element name="climate">
      <ref name="attlist.climate"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="model.labelLike"/>
        </oneOrMore>
      </choice>
      <zeroOrMore>
        <choice>
          <ref name="model.noteLike"/>
          <ref name="model.biblLike"/>
        </choice>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="climate"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.climate" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.naming.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:groups together salutations, datelines, and similar phrases appearing as a final group at
    the end of a division, especially of a letter.
  -->
  <define name="closer">
    <element name="closer">
      <ref name="attlist.closer"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="signed"/>
          <ref name="dateline"/>
          <ref name="salute"/>
          <ref name="model.phrase"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.closer" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains a description of how the leaves or bifolia are physically
    arranged.
  -->
  <define name="collation">
    <element name="collation">
      <ref name="attlist.collation"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.collation" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains the name of a collection of
    manuscripts, not necessarily located within a single repository.
  -->
  <define name="collection">
    <element name="collection">
      <ref name="attlist.collection"/>
      <ref name="macro.xtext"/>
    </element>
  </define>
  <define name="attlist.collection" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.naming.attributes"/>
  </define>
  <!--
    doc:contains the  colophon
    of a  manuscript item: that is, a statement providing information regarding the date, place, agency, or reason for production of the manuscript.
  -->
  <define name="colophon">
    <element name="colophon">
      <ref name="attlist.colophon"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.colophon" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains a description of the physical
    condition of the manuscript.
  -->
  <define name="condition">
