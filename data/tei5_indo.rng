    <optional>
      <attribute name="degree"/>
    </optional>
  </define>
  <!--
    doc:(separated from the surrounding text with quotation marks) contains material which is marked as (ostensibly) being somehow different than the
    surrounding text, for any one of a variety of reasons including, but not limited to: direct
    speech or thought, technical terms or jargon, authorial distance, quotations from elsewhere, and
    passages that are mentioned but not used.
  -->
  <define name="q">
    <element name="q">
      <ref name="attlist.q"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.q" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.ascribed.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:specifies editorial practice adopted with respect to quotation
    marks in the original.
  -->
  <define name="quotation">
    <element name="quotation">
      <ref name="attlist.quotation"/>
      <oneOrMore>
        <ref name="model.pLike"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.quotation" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
    <optional>
      <attribute name="marks" a:defaultValue="all">
        <choice>
          <value>none</value>
          <value>some</value>
          <value>all</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="form" a:defaultValue="unknown"/>
    </optional>
  </define>
  <!--
    doc:(quotation) contains a phrase or passage attributed by the narrator or author to some agency external
    to the text.
  -->
  <define name="quote">
    <element name="quote">
      <ref name="attlist.quote"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.quote" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.msExcerpt.attributes"/>
  </define>
  <!-- doc:(reading) contains a single reading within a textual variation. -->
  <define name="rdg">
    <element name="rdg">
      <ref name="attlist.rdg"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="model.phrase"/>
          <ref name="model.inter"/>
          <ref name="model.global"/>
          <ref name="model.rdgPart"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.rdg" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.textCritical.attributes"/>
  </define>
  <!--
    doc:(reading group) within a textual variation,
    groups two or more readings perceived to have a genetic
    relationship or other affinity.
  -->
  <define name="rdgGrp">
    <element name="rdgGrp">
      <ref name="attlist.rdgGrp"/>
      <oneOrMore>
        <choice>
          <group>
            <ref name="rdgGrp"/>
            <optional>
              <ref name="wit"/>
            </optional>
          </group>
          <zeroOrMore>
            <optional>
              <ref name="lem"/>
              <optional>
                <ref name="wit"/>
              </optional>
            </optional>
            <group>
              <ref name="model.rdgLike"/>
              <optional>
                <ref name="wit"/>
              </optional>
            </group>
          </zeroOrMore>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.rdgGrp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.textCritical.attributes"/>
  </define>
  <!--
    doc:(recorded history) provides information about the source and
    revision status of the parent manuscript description itself.
  -->
  <define name="recordHist">
    <element name="recordHist">
      <ref name="attlist.recordHist"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <ref name="source"/>
          <zeroOrMore>
            <ref name="change"/>
          </zeroOrMore>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.recordHist" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:(reference) defines a reference to another location, possibly modified by additional text or comment. -->
  <define name="ref">
    <element name="ref">
      <ref name="attlist.ref"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.ref" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.pointing.attributes"/>
    <ref name="att.declaring.attributes"/>
    <optional>
      <attribute name="target"/>
    </optional>
    <optional>
      <attribute name="cRef"/>
    </optional>
  </define>
  <!--
    doc:(reference state) specifies one component of a canonical reference defined by
    the milestone method.
  -->
  <define name="refState">
    <element name="refState">
      <ref name="attlist.refState"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.refState" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.sourced.attributes"/>
    <attribute name="unit">
      <data type="NMTOKEN"/>
    </attribute>
    <optional>
      <attribute name="length"/>
    </optional>
    <optional>
      <attribute name="delim"/>
    </optional>
  </define>
  <!--
    doc:(references declaration) specifies how canonical references are constructed for this
    text.
  -->
  <define name="refsDecl">
    <element name="refsDecl">
      <ref name="attlist.refsDecl"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="cRefPattern"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="refState"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.refsDecl" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!-- doc:(regularization) contains a reading which has been regularized or normalized in some sense. -->
  <define name="reg">
    <element name="reg">
      <ref name="attlist.reg"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.reg" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:contains the name of an administrative unit such as a state, province, or county, larger
    than a settlement, but smaller than a country.
  -->
  <define name="region">
    <element name="region">
      <ref name="attlist.region"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.region" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.naming.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.datable.attributes"/>
  </define>
  <!--
    doc:contains or references some other bibliographic item which is related to the present one
    in some specified manner, for example as a constituent or alternative version of it.
  -->
  <define name="relatedItem">
    <element name="relatedItem">
      <ref name="attlist.relatedItem"/>
      <choice>
        <ref name="model.biblLike"/>
        <ref name="model.ptrLike"/>
      </choice>
    </element>
  </define>
  <define name="attlist.relatedItem" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(relationship) describes any kind of relationship or linkage amongst a specified
    group of participants.
  -->
  <define name="relation">
    <element name="relation">
      <ref name="attlist.relation"/>
      <optional>
        <ref name="desc"/>
      </optional>
    </element>
  </define>
  <define name="attlist.relation" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.naming.attributes"/>
    <optional>
      <attribute name="type" a:defaultValue="personal">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <attribute name="name">
      <data type="NMTOKEN"/>
    </attribute>
    <optional>
      <attribute name="passive"/>
    </optional>
    <optional>
      <attribute name="active"/>
    </optional>
    <optional>
      <attribute name="mutual"/>
    </optional>
  </define>
  <!--
    doc:(relation group) provides information about relationships identified amongst people, places, and
    organizations, either informally as prose or as formally expressed relation links.
  -->
  <define name="relationGrp">
    <element name="relationGrp">
      <ref name="attlist.relationGrp"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="relation"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.relationGrp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:supplies information about the rendition or appearance of one or more
    elements in the source text.
  -->
  <define name="rendition">
    <element name="rendition">
      <ref name="attlist.rendition"/>
      <ref name="macro.limitedContent"/>
    </element>
  </define>
  <define name="attlist.rendition" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="scheme">
        <choice>
          <value>css</value>
          <value>xslfo</value>
          <value>free</value>
          <value>other</value>
        </choice>
      </attribute>
    </optional>
  </define>
  <!-- doc:contains the name of a repository within which manuscripts are stored, possibly forming part of an institution. -->
  <define name="repository">
    <element name="repository">
      <ref name="attlist.repository"/>
      <ref name="macro.xtext"/>
    </element>
  </define>
  <define name="attlist.repository" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.naming.attributes"/>
  </define>
  <!-- doc:(residence) describes a person's present or past places of residence. -->
  <define name="residence">
    <element name="residence">
      <ref name="attlist.residence"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.residence" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.naming.attributes"/>
  </define>
  <!-- doc:(responsibility) contains a phrase describing the nature of a person's intellectual responsibility. -->
  <define name="resp">
    <element name="resp">
      <ref name="attlist.resp"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.resp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.canonical.attributes"/>
  </define>
  <!--
    doc:(statement of responsibility) supplies a statement of responsibility for the intellectual content of a text, edition,
    recording, or series, where the specialized elements for authors, editors, etc. do not suffice
    or do not apply.
  -->
  <define name="respStmt">
    <element name="respStmt">
      <ref name="attlist.respStmt"/>
      <choice>
        <group>
          <oneOrMore>
            <ref name="resp"/>
          </oneOrMore>
          <oneOrMore>
            <ref name="model.nameLike.agent"/>
          </oneOrMore>
        </group>
        <group>
          <oneOrMore>
            <ref name="model.nameLike.agent"/>
          </oneOrMore>
          <oneOrMore>
            <ref name="resp"/>
          </oneOrMore>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.respStmt" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(responsibility) identifies the individual(s) responsible for some aspect of the
    markup of particular element(s).
  -->
  <define name="respons">
    <element name="respons">
      <ref name="attlist.respons"/>
      <zeroOrMore>
        <ref name="model.glossLike"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.respons" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="target"/>
    <attribute name="locus">
      <data type="NMTOKEN"/>
    </attribute>
    <attribute name="resp"/>
  </define>
  <!--
    doc:indicates restoration of text to an earlier state by
    cancellation of an editorial or authorial marking or instruction.
  -->
  <define name="restore">
    <element name="restore">
      <ref name="attlist.restore"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.restore" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.transcriptional.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!-- doc:(revision description) summarizes the revision history for a file. -->
  <define name="revisionDesc">
    <element name="revisionDesc">
      <ref name="attlist.revisionDesc"/>
      <choice>
        <ref name="list"/>
        <oneOrMore>
          <ref name="change"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.revisionDesc" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:marks the rhyming part of  a metrical line. -->
  <define name="rhyme">
    <element name="rhyme">
      <ref name="attlist.rhyme"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.rhyme" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <optional>
      <attribute name="label"/>
    </optional>
  </define>
  <!--
    doc:contains a name component which indicates that the referent has a particular role or
    position in society, such as an official title or rank.
  -->
  <define name="roleName">
    <element name="roleName">
      <ref name="attlist.roleName"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.roleName" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.personal.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!-- doc:contains one row of a table. -->
  <define name="row">
    <element name="row">
      <ref name="attlist.row"/>
      <oneOrMore>
        <ref name="cell"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.row" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.tableDecoration.attributes"/>
  </define>
  <!-- doc:(referencing string) contains a general purpose name or referring string. -->
  <define name="rs">
    <element name="rs">
      <ref name="attlist.rs"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.rs" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.naming.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:contains the text of any rubric or heading attached to a particular manuscript item, that is, a string of words through which a
    manuscript signals the beginning of a text division, often with an assertion as to its author and title, which is in some way set off from the text itself, usually in red ink, or by use of different size or type of script, or some other such visual device.
  -->
  <define name="rubric">
    <element name="rubric">
      <ref name="attlist.rubric"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.rubric" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!-- doc:(s-unit) contains a sentence-like division of a text. -->
  <define name="s">
    <element name="s">
      <ref name="attlist.s"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.s" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.segLike.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(speech or thought) indicates passages thought or spoken aloud, whether
    explicitly indicated in the source or not, whether directly
    or indirectly reported, whether by real people or
    fictional characters.
  -->
  <define name="said">
    <element name="said">
      <ref name="attlist.said"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.said" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.ascribed.attributes"/>
    <optional>
      <attribute name="aloud" a:defaultValue="unknown"/>
    </optional>
    <optional>
      <attribute name="direct" a:defaultValue="true"/>
    </optional>
  </define>
  <!--
    doc:(salutation) contains a salutation or greeting prefixed to a foreword, dedicatory epistle, or other
    division of a text, or the salutation in the closing of a letter, preface, etc.
  -->
  <define name="salute">
    <element name="salute">
      <ref name="attlist.salute"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.salute" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(sampling declaration) contains a prose description of the rationale and methods used
    in sampling texts in the creation of a corpus or collection.
  -->
  <define name="samplingDecl">
    <element name="samplingDecl">
      <ref name="attlist.samplingDecl"/>
      <oneOrMore>
        <ref name="model.pLike"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.samplingDecl" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:contains a description of one seal or similar
    attachment applied to a manuscript.
  -->
  <define name="seal">
    <element name="seal">
      <ref name="attlist.seal"/>
      <oneOrMore>
        <choice>
          <ref name="model.pLike"/>
          <ref name="decoNote"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.seal" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.datable.attributes"/>
    <optional>
      <attribute name="contemporary"/>
    </optional>
  </define>
  <!--
    doc:(seal description) describes the seals or other external items attached to a manuscript, either
    as a series of paragraphs or as a series of distinct seal elements,
    possibly with additional decoNotes.
  -->
  <define name="sealDesc">
    <element name="sealDesc">
      <ref name="attlist.sealDesc"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <choice>
            <ref name="decoNote"/>
            <ref name="seal"/>
            <ref name="condition"/>
          </choice>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.sealDesc" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(second folio) The word or words taken from a fixed point
    in a codex (typically the beginning of the
    second leaf) in order
    to provide a unique identifier for it.
  -->
  <define name="secFol">
    <element name="secFol">
      <ref name="attlist.secFol"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.secFol" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(arbitrary segment) represents any  segmentation of text below the
    chunk level.
  -->
  <define name="seg">
    <element name="seg">
      <ref name="attlist.seg"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.seg" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.segLike.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:describes the principles according to which the text has been
    segmented, for example into sentences, tone-units, graphemic strata,
    etc.
  -->
  <define name="segmentation">
    <element name="segmentation">
      <ref name="attlist.segmentation"/>
      <oneOrMore>
        <ref name="model.pLike"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.segmentation" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:(series information) contains information about the series in which a book or other bibliographic item has
    appeared.
  -->
  <define name="series">
    <element name="series">
      <ref name="attlist.series"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="title"/>
          <ref name="editor"/>
          <ref name="respStmt"/>
          <ref name="biblScope"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.series" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(series statement) groups information about the series, if any, to which
    a publication belongs.
  -->
  <define name="seriesStmt">
    <element name="seriesStmt">
      <ref name="attlist.seriesStmt"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <oneOrMore>
            <ref name="title"/>
          </oneOrMore>
          <zeroOrMore>
            <choice>
              <ref name="idno"/>
              <ref name="respStmt"/>
            </choice>
          </zeroOrMore>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.seriesStmt" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:describes one particular setting in which a language
    interaction takes place.
  -->
  <define name="setting">
    <element name="setting">
      <ref name="attlist.setting"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <zeroOrMore>
          <choice>
            <ref name="model.nameLike.agent"/>
            <ref name="model.dateLike"/>
            <ref name="model.settingPart"/>
          </choice>
        </zeroOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.setting" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.ascribed.attributes"/>
  </define>
  <!--
    doc:(setting description) describes the setting or settings within which a language
    interaction takes place, either as a prose description or as a
    series of setting elements.
  -->
  <define name="settingDesc">
    <element name="settingDesc">
      <ref name="attlist.settingDesc"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="setting"/>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.settingDesc" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:contains the name of a settlement such as a city, town, or village identified as a single
    geo-political or administrative unit.
  -->
  <define name="settlement">
    <element name="settlement">
      <ref name="attlist.settlement"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.settlement" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.naming.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.datable.attributes"/>
  </define>
  <!-- doc:specifies the sex of a person. -->
  <define name="sex">
    <element name="sex">
      <ref name="attlist.sex"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.sex" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.datable.attributes"/>
    <optional>
      <attribute name="value"/>
    </optional>
  </define>
  <!-- doc:(latin for thus or so) contains text reproduced although apparently incorrect or inaccurate. -->
  <define name="sic">
    <element name="sic">
      <ref name="attlist.sic"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.sic" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:contains discussion of the leaf or quire signatures found within a codex. -->
  <define name="signatures">
    <element name="signatures">
      <ref name="attlist.signatures"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.signatures" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(signature) contains the closing salutation, etc., appended to a foreword,
    dedicatory epistle, or other division of a text.
  -->
  <define name="signed">
    <element name="signed">
      <ref name="attlist.signed"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.signed" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains a word or phrase for which the author or narrator
    indicates a disclaiming of responsibility, for example by the use
    of scare quotes or italics.
  -->
  <define name="soCalled">
    <element name="soCalled">
      <ref name="attlist.soCalled"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.soCalled" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(socio-economic status) contains an informal description of a person's perceived social or
    economic status.
  -->
  <define name="socecStatus">
    <element name="socecStatus">
      <ref name="attlist.socecStatus"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.socecStatus" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.naming.attributes"/>
    <optional>
      <attribute name="scheme"/>
    </optional>
    <optional>
      <attribute name="code"/>
    </optional>
  </define>
  <!-- doc:describes the original source for the information contained with a manuscript description. -->
  <define name="source">
    <element name="source">
      <ref name="attlist.source"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.source" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(source description) describes the source from
    which an electronic text was derived or generated, typically a bibliographic description in the case of a digitized text, or a phrase such as "born digital" for a text which has no previous existence.
  -->
  <define name="sourceDesc">
    <element name="sourceDesc">
      <ref name="attlist.sourceDesc"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <choice>
            <ref name="model.biblLike"/>
            <ref name="_DUMMY_model.sourceDescPart"/>
            <ref name="model.listLike"/>
          </choice>
        </oneOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.sourceDesc" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:(speech) An individual speech in a performance text, or a passage presented as such in a prose or
    verse text.
  -->
  <define name="sp">
    <element name="sp">
      <ref name="attlist.sp"/>
      <zeroOrMore>
        <ref name="model.global"/>
      </zeroOrMore>
      <optional>
        <ref name="speaker"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </optional>
      <oneOrMore>
        <choice>
          <ref name="model.lLike"/>
          <ref name="lg"/>
          <ref name="model.pLike"/>
          <ref name="model.stageLike"/>
          <ref name="model.qLike"/>
        </choice>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.sp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.ascribed.attributes"/>
  </define>
  <!-- doc:indicates the location of a significant space in the copy text. -->
  <define name="space">
    <element name="space">
      <ref name="attlist.space"/>
      <empty/>
    </element>
  </define>
  <define name="attlist.space" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.dimensions.attributes"/>
    <optional>
      <attribute name="dim">
        <choice>
          <value>horizontal</value>
          <value>vertical</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="resp"/>
    </optional>
  </define>
  <!--
    doc:associates an interpretative annotation directly with a span
    of text.
  -->
  <define name="span">
    <element name="span">
      <ref name="attlist.span"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.span" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.interpLike.attributes"/>
    <attribute name="from"/>
    <optional>
      <attribute name="to"/>
    </optional>
  </define>
  <!-- doc:(span group) collects together span tags. -->
  <define name="spanGrp">
    <element name="spanGrp">
      <ref name="attlist.spanGrp"/>
      <zeroOrMore>
        <ref name="span"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.spanGrp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.interpLike.attributes"/>
  </define>
  <!--
    doc:A specialized form of heading or label, giving the name of one or more speakers in a
    dramatic text or fragment.
  -->
  <define name="speaker">
    <element name="speaker">
      <ref name="attlist.speaker"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.speaker" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:specifies the name of a sponsoring organization or
    institution.
  -->
  <define name="sponsor">
    <element name="sponsor">
      <ref name="attlist.sponsor"/>
      <ref name="macro.phraseSeq.limited"/>
    </element>
  </define>
  <define name="attlist.sponsor" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:(stage direction) contains any kind of stage direction within a dramatic text or fragment. -->
  <define name="stage">
    <element name="stage">
      <ref name="attlist.stage"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.stage" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:contains a word or phrase describing a
    stamp or similar device.
  -->
  <define name="stamp">
    <element name="stamp">
      <ref name="attlist.stamp"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.stamp" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.datable.attributes"/>
  </define>
  <!--
    doc:contains a description of some status or quality
    attributed to a person, place, or organization at some specific time.
  -->
  <define name="state">
    <element name="state">
      <ref name="attlist.state"/>
      <choice>
        <oneOrMore>
          <ref name="state"/>
        </oneOrMore>
        <group>
          <zeroOrMore>
            <ref name="model.headLike"/>
          </zeroOrMore>
          <oneOrMore>
            <ref name="model.pLike"/>
          </oneOrMore>
          <zeroOrMore>
            <choice>
              <ref name="model.noteLike"/>
              <ref name="model.biblLike"/>
            </choice>
          </zeroOrMore>
        </group>
        <zeroOrMore>
          <choice>
            <ref name="model.labelLike"/>
            <ref name="model.noteLike"/>
            <ref name="model.biblLike"/>
          </choice>
        </zeroOrMore>
      </choice>
    </element>
  </define>
  <define name="attlist.state" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.naming.attributes"/>
  </define>
  <!--
    doc:(standard values) specifies the format used when standardized date or number
    values are supplied.
  -->
  <define name="stdVals">
    <element name="stdVals">
      <ref name="attlist.stdVals"/>
      <oneOrMore>
        <ref name="model.pLike"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.stdVals" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:a full street address including any name or number identifying a
    building as well as the name of the street or route on which it is
    located.
  -->
  <define name="street">
    <element name="street">
      <ref name="attlist.street"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.street" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:(substitution) groups one or more deletions with one or more additions when
    the combination is to be regarded as a single intervention in the text.
  -->
  <define name="subst">
    <element name="subst">
      <ref name="attlist.subst"/>
      <ref name="model.pPart.transcriptional"/>
      <oneOrMore>
        <ref name="model.pPart.transcriptional"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.subst" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.transcriptional.attributes"/>
  </define>
  <!--
    doc:contains a brief summary of the
    intellectual content of an item, provided by the cataloguer.
  -->
  <define name="summary">
    <element name="summary">
      <ref name="attlist.summary"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.summary" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:signifies text supplied by the transcriber or editor for any
    reason, typically because the original cannot be read because of
    physical damage or loss to the original.
  -->
  <define name="supplied">
    <element name="supplied">
      <ref name="attlist.supplied"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.supplied" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.editLike.attributes"/>
    <optional>
      <attribute name="reason"/>
    </optional>
  </define>
  <!--
    doc:contains a description of the materials
    etc. which make up the physical support for the written part of a manuscript.
  -->
  <define name="support">
    <element name="support">
      <ref name="attlist.support"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.support" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:(support description) groups elements describing the physical support for the written part of a manuscript. -->
  <define name="supportDesc">
    <element name="supportDesc">
      <ref name="attlist.supportDesc"/>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <group>
          <optional>
            <ref name="support"/>
          </optional>
          <optional>
            <ref name="extent"/>
          </optional>
          <zeroOrMore>
            <ref name="foliation"/>
          </zeroOrMore>
          <optional>
            <ref name="collation"/>
          </optional>
          <optional>
            <ref name="condition"/>
          </optional>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.supportDesc" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="material">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:defines a written surface in terms of a rectangular
    coordinate space, optionally  grouping one or more graphic representations of
    that space, and rectangular zones of interest
    within it.
  -->
  <define name="surface">
    <element name="surface">
      <ref name="attlist.surface"/>
      <zeroOrMore>
        <ref name="model.glossLike"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="model.graphicLike"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="zone"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.surface" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.coordinated.attributes"/>
    <ref name="att.declaring.attributes"/>
    <optional>
      <attribute name="start"/>
    </optional>
  </define>
  <!--
    doc:contains a family (inherited) name, as opposed to a given,
    baptismal, or nick name.
  -->
  <define name="surname">
    <element name="surname">
      <ref name="attlist.surname"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.surname" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.personal.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:contains information about any digital or
    photographic representations of the manuscript being described which
    may exist in the holding institution or elsewhere.
  -->
  <define name="surrogates">
    <element name="surrogates">
      <ref name="attlist.surrogates"/>
      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.surrogates" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!-- doc:contains text displayed in tabular form, in rows and columns. -->
  <define name="table">
    <element name="table">
      <ref name="attlist.table"/>
      <zeroOrMore>
        <choice>
          <ref name="model.headLike"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
      <oneOrMore>
        <ref name="row"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.table" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="rows"/>
    </optional>
    <optional>
      <attribute name="cols"/>
    </optional>
  </define>
  <!--
    doc:supplies information about the usage of a specific element
    within a text.
  -->
  <define name="tagUsage">
    <element name="tagUsage">
      <ref name="attlist.tagUsage"/>
      <ref name="macro.limitedContent"/>
    </element>
  </define>
  <define name="attlist.tagUsage" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="gi">
      <data type="NMTOKEN"/>
    </attribute>
    <optional>
      <attribute name="occurs"/>
    </optional>
    <optional>
      <attribute name="withId"/>
    </optional>
    <optional>
      <attribute name="render"/>
    </optional>
  </define>
  <!-- doc:(tagging declaration) provides detailed information about the tagging applied to a document. -->
  <define name="tagsDecl">
    <element name="tagsDecl">
      <ref name="attlist.tagsDecl"/>
      <zeroOrMore>
        <ref name="rendition"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="namespace"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.tagsDecl" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:defines a typology used to classify texts either implicitly, by
    means of a bibliographic citation, or explicitly by a structured
    taxonomy.
  -->
  <define name="taxonomy">
    <element name="taxonomy">
      <ref name="attlist.taxonomy"/>
      <choice>
        <zeroOrMore>
          <ref name="model.glossLike"/>
        </zeroOrMore>
        <oneOrMore>
          <ref name="category"/>
        </oneOrMore>
        <group>
          <ref name="model.biblLike"/>
          <zeroOrMore>
            <ref name="category"/>
          </zeroOrMore>
        </group>
      </choice>
    </element>
  </define>
  <define name="attlist.taxonomy" combine="interleave">
    <ref name="att.global.attributes"/>
  </define>
  <!--
    doc:contains the whole of a TEI encoded corpus, comprising a single corpus header and one or
    more TEI elements, each containing a single text header and a text.
  -->
  <define name="teiCorpus">
    <element name="teiCorpus">
      <ref name="attlist.teiCorpus"/>
      <ref name="teiHeader"/>
      <oneOrMore>
        <choice>
          <ref name="TEI"/>
          <ref name="teiCorpus"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.teiCorpus" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="version" a:defaultValue="5.0"/>
    </optional>
  </define>
  <define name="attlist.teiCorpus" combine="interleave">
    <optional>
      <attribute name="xsi:schemaLocation"/>
    </optional>
  </define>
  <!--
    doc:(TEI Header) supplies the descriptive and declarative information making
    up an electronic title page prefixed to every TEI-conformant
    text.
  -->
  <define name="teiHeader">
    <element name="teiHeader">
      <ref name="attlist.teiHeader"/>
      <ref name="fileDesc"/>
      <zeroOrMore>
        <ref name="model.headerPart"/>
      </zeroOrMore>
      <optional>
        <ref name="revisionDesc"/>
      </optional>
    </element>
  </define>
  <define name="attlist.teiHeader" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="type" a:defaultValue="text">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
  <!--
    doc:contains a single-word, multi-word, or symbolic designation which is regarded as a
    technical term.
  -->
  <define name="term">
    <element name="term">
      <ref name="attlist.term"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.term" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declaring.attributes"/>
    <ref name="att.typed.attributes"/>
    <ref name="att.canonical.attributes"/>
    <optional>
      <attribute name="sortKey"/>
    </optional>
    <optional>
      <attribute name="target"/>
    </optional>
    <optional>
      <attribute name="cRef"/>
    </optional>
  </define>
  <!-- doc:contains information about the physical terrain of a place. -->
  <define name="terrain">
    <element name="terrain">
      <ref name="attlist.terrain"/>
      <zeroOrMore>
        <ref name="model.headLike"/>
      </zeroOrMore>
      <choice>
        <oneOrMore>
          <ref name="model.pLike"/>
        </oneOrMore>
        <oneOrMore>
          <ref name="model.labelLike"/>
        </oneOrMore>
      </choice>
      <zeroOrMore>
        <choice>
          <ref name="model.noteLike"/>
          <ref name="model.biblLike"/>
        </choice>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="terrain"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.terrain" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.naming.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:contains a single text of any kind, whether unitary or composite, for example a poem or
    drama, a collection of essays, a novel, a dictionary, or a corpus sample.
  -->
  <define name="text">
    <element name="text">
      <ref name="attlist.text"/>
      <zeroOrMore>
        <ref name="model.global"/>
      </zeroOrMore>
      <optional>
        <ref name="front"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </optional>
      <choice>
        <ref name="body"/>
        <ref name="group"/>
      </choice>
      <zeroOrMore>
        <ref name="model.global"/>
      </zeroOrMore>
      <optional>
        <ref name="back"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </optional>
    </element>
  </define>
  <define name="attlist.text" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declaring.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(text classification) groups information which describes the nature or topic of a text
    in terms of a standard classification scheme, thesaurus, etc.
  -->
  <define name="textClass">
    <element name="textClass">
      <ref name="attlist.textClass"/>
      <zeroOrMore>
        <choice>
          <ref name="classCode"/>
          <ref name="catRef"/>
          <ref name="keywords"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.textClass" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:(text description) provides a description of a text in terms of its
    situational parameters.
  -->
  <define name="textDesc">
    <element name="textDesc">
      <ref name="attlist.textDesc"/>
      <ref name="model.textDescPart_sequence"/>
      <oneOrMore>
        <ref name="purpose"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.textDesc" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.declarable.attributes"/>
  </define>
  <!--
    doc:(text language) describes the languages and writing systems used by a
    manuscript (as opposed to its description, which is described in the langUsage element).
  -->
  <define name="textLang">
    <element name="textLang">
      <ref name="attlist.textLang"/>
      <ref name="macro.phraseSeq"/>
    </element>
  </define>
  <define name="attlist.textLang" combine="interleave">
    <ref name="att.global.attributes"/>
    <optional>
      <attribute name="mainLang"/>
    </optional>
    <optional>
      <attribute name="otherLangs"/>
    </optional>
  </define>
  <!-- doc:contains a phrase defining a time of day in any format. -->
  <define name="time">
    <element name="time">
      <ref name="attlist.time"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="_DUMMY_model.gLike"/>
          <ref name="model.phrase"/>
          <ref name="model.global"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.time" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.datable.attributes"/>
    <ref name="att.editLike.attributes"/>
    <ref name="att.typed.attributes"/>
  </define>
  <!--
    doc:(timeline) provides a set of ordered points in time which can be linked to
    elements of a spoken text to create a temporal alignment of that text.
  -->
  <define name="timeline">
    <element name="timeline">
      <ref name="attlist.timeline"/>
      <oneOrMore>
        <ref name="when"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.timeline" combine="interleave">
    <ref name="att.global.attributes"/>
    <attribute name="origin"/>
    <optional>
      <attribute name="unit">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="interval"/>
    </optional>
  </define>
  <!-- doc:contains a title for any kind of work. -->
  <define name="title">
    <element name="title">
      <ref name="attlist.title"/>
      <ref name="macro.paraContent"/>
    </element>
  </define>
  <define name="attlist.title" combine="interleave">
    <ref name="att.global.attributes"/>
    <ref name="att.canonical.attributes"/>
    <optional>
      <attribute name="level">
        <choice>
          <value>a</value>
          <value>m</value>
          <value>j</value>
          <value>s</value>
          <value>u</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="type">
        <data type="NMTOKEN"/>
      </attribute>
    </optional>
  </define>
